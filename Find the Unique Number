// Make sure your class is public
 public class Kata {
    public static double findUniq(double arr[]) {
      double repeat;
      if(arr[0]==arr[1]) {
        repeat=arr[0];
        for(Double let:arr) {
          if(let!=repeat)
            return let;
        }
      }
      else {
        if(arr[1]==arr[2])
          return arr[0];
        else
          return arr[1];
      }
      return 0;
    }
}
